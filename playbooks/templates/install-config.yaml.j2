apiVersion: v1
baseDomain: devcluster.openshift.com
metadata:
  name: {{ cluster_id }}
platform:
  aws:
    region: {{ aws_region }}
pullSecret: '{{ pull_secret['content'] | b64decode | from_json | to_json(separators=(',',':')) }}'
sshKey: |
  {{ keygen_output.stdout }}
{% if fips_enable is defined and fips_enable.lower() == 'true' %}
fips: true
{% endif %}
